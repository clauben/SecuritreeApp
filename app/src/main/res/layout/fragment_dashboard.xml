<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/Pistachio"
    tools:context=".ui.dashboard.DashboardFragment">



    <ImageView
        android:id="@+id/imageView"
        android:layout_width="142dp"
        android:layout_height="165dp"
        android:layout_marginTop="32dp"
        android:contentDescription="@string/login_logo"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/tree_logo_round" />

    <ImageView
        android:id="@+id/dashUI"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView"
        app:srcCompat="@drawable/dashboard2" />

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="108dp"
        android:layout_height="85dp"
        android:layout_marginEnd="30dp"
        android:layout_marginBottom="175dp"
        app:layout_constraintBottom_toBottomOf="@+id/dashUI"
        app:layout_constraintEnd_toEndOf="@+id/dashUI"
        app:layout_constraintTop_toTopOf="@+id/dashUI"
        app:srcCompat="@drawable/ic_pin" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="117dp"
        android:layout_height="109dp"
        android:layout_marginStart="30dp"
        android:layout_marginBottom="175dp"
        app:layout_constraintBottom_toBottomOf="@+id/dashUI"
        app:layout_constraintStart_toStartOf="@+id/dashUI"
        app:layout_constraintTop_toTopOf="@+id/dashUI"
        app:srcCompat="@drawable/ic_person" />

    <ImageView
        android:layout_width="108dp"
        android:layout_height="93dp"
        android:layout_marginTop="175dp"
        android:layout_marginEnd="30dp"
        app:layout_constraintBottom_toBottomOf="@+id/dashUI"
        app:layout_constraintEnd_toEndOf="@+id/dashUI"
        app:layout_constraintTop_toTopOf="@+id/dashUI"
        app:srcCompat="@drawable/ic_settings" />

    <ImageView
        android:layout_width="111dp"
        android:layout_height="87dp"
        android:layout_marginStart="35dp"
        android:layout_marginTop="175dp"
        app:layout_constraintBottom_toBottomOf="@+id/dashUI"
        app:layout_constraintStart_toStartOf="@+id/dashUI"
        app:layout_constraintTop_toTopOf="@+id/dashUI"
        app:srcCompat="@drawable/ic_forum" />
        


</androidx.constraintlayout.widget.ConstraintLayout>